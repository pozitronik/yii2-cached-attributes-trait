{
	"name": "pozitronik/yii2-cached-properties-trait",
	"description": "Support to cache yii2 models properties",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"extension",
		"cache"
	],
	"license": "GPL-3.0-or-later",
	"minimum-stability": "dev",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0",
		"yiisoft/yii2": "^2.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.3",
		"codeception/module-yii2": "^1.1",
		"vlucas/phpdotenv": "^5.4.1",
		"yiisoft/yii2-bootstrap4": "~2.0.10"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		},
		"github-protocols": [
			"https"
		],
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"autoload": {
		"psr-4": {
			"pozitronik\\cached_properties\\": "src"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
